<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="380.0" prefWidth="470.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.AddCourseAddFirstController">
   <children>
      <Pane prefHeight="59.0" prefWidth="560.0" style="-fx-background-color: #928a37;">
         <children>
            <Label layoutX="119.0" layoutY="12.0" text="Add Courses (Add First)">
               <font>
                  <Font name="Arial" size="30.0" />
               </font>
            </Label>
         </children>
      </Pane>
      <Pane layoutY="59.0" prefHeight="226.0" prefWidth="560.0" style="-fx-background-color: #c6bb66;">
         <children>
            <Label layoutX="46.0" layoutY="44.0" text="Course ID:">
               <font>
                  <Font name="Arial Bold" size="18.0" />
               </font>
            </Label>
            <TextField fx:id="textFieldCourseId" layoutX="152.0" layoutY="30.0" prefHeight="48.0" prefWidth="149.0">
               <font>
                  <Font name="Arial" size="18.0" />
               </font>
            </TextField>
            <Label layoutX="47.0" layoutY="102.0" text="Name:">
               <font>
                  <Font name="Arial Bold" size="18.0" />
               </font>
            </Label>
            <TextField fx:id="textFieldName" layoutX="153.0" layoutY="89.0" prefHeight="48.0" prefWidth="387.0">
               <font>
                  <Font name="Arial" size="18.0" />
               </font>
            </TextField>
            <Label layoutX="46.0" layoutY="159.0" text="Credits:">
               <font>
                  <Font name="Arial Bold" size="18.0" />
               </font>
            </Label>
            <TextField fx:id="textFieldCredits" layoutX="152.0" layoutY="146.0" prefHeight="48.0" prefWidth="149.0">
               <font>
                  <Font name="Arial" size="18.0" />
               </font>
            </TextField>
         </children>
      </Pane>
      <Pane layoutY="285.0" prefHeight="95.0" prefWidth="560.0" style="-fx-background-color: #aca34e;">
         <children>
            <Button layoutX="14.0" layoutY="18.0" mnemonicParsing="false" onAction="#addCourseOnAction" prefHeight="59.0" prefWidth="134.0" text="Add">
               <font>
                  <Font name="Arial Bold" size="18.0" />
               </font>
            </Button>
            <Button layoutX="213.0" layoutY="18.0" mnemonicParsing="false" onAction="#cleanCourseOnAction" prefHeight="59.0" prefWidth="134.0" text="Clean">
               <font>
                  <Font name="Arial Bold" size="18.0" />
               </font>
            </Button>
            <Button layoutX="400.0" layoutY="18.0" mnemonicParsing="false" onAction="#closeCourseOnAction" prefHeight="59.0" prefWidth="134.0" text="Close">
               <font>
                  <Font name="Arial Bold" size="18.0" />
               </font>
            </Button>
         </children>
      </Pane>
   </children>
</AnchorPane>
